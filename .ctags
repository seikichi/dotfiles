--langdef=typescript
--langmap=typescript:.ts
--regex-typescript=/^[ \t]*(export)?[ \t]*class[ \t]+([a-zA-Z0-9_]+)/\2/c,classes/
--regex-typescript=/^[ \t]*(export)?[ \t]*module[ \t]+([a-zA-Z0-9_]+\.)*([a-zA-Z0-9_]+)/\3/n,modules/
--regex-typescript=/^[ \t]*(export)?[ \t]*function[ \t]+([a-zA-Z0-9_]+)/\2/f,functions/
--regex-typescript=/^[ \t]*(export)?[ \t]*var[ \t]+([$a-zA-Z0-9_]+)/\2/v,variables/
--regex-typescript=/(public|private)[ \t]+([$a-zA-Z0-9_]+)/\3/m,members/
--regex-typescript=/^[ \t]*(export)?[ \t]*([$a-zA-Z0-9_]+)([ \t]*\?)?[ \t]*:.*;$/\2/m,members/
--regex-typescript=/^[ \t]*(export)?[ \t]*([$a-zA-Z0-9_]+)(<([a-zA-Z0-9_]+[ \t]*,[ \t]*)*[a-zA-Z0-9_]+>)?[ \t]*\(\)[ \t]*:/\2/m,members/
--regex-typescript=/^[ \t]*(export)?[ \t]*([$a-zA-Z0-9_]+)(<([a-zA-Z0-9_]+[ \t]*,[ \t]*)*[a-zA-Z0-9_]+>)?[ \t]*\([a-zA-Z0-9_]+[ \t]*:/\2/m,members/
--regex-typescript=/^[ \t]*(export)?[ \t]*interface[ \t]+([a-zA-Z0-9_]+)/\2/i,interfaces/
--regex-typescript=/^[ \t]*(export)?[ \t]*enum[ \t]+([a-zA-Z0-9_]+)/\2/e,enums/

--regex-javascript=/^[ \t]*(['"]?)([A-Za-z0-9_.]+)\1[ \t]*[:=][ \t]*function[ \t]*\(/\2/I,inner/i
